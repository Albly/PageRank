import numpy as np

def mcmc(n_trials, A):
    page = np.random.randint(A.shape[0])
    n = A.shape[0]

    freqs = np.zeros(n)

    for i in range(n_trials):  
        page = np.random.choice(np.arange(A.shape[0]).tolist() , 1, p = A[:,page].tolist())[0]
        freqs[page] += 1
    
    ans = freqs/n_trials
    ans = ans/np.linalg.norm(ans)

    return ans